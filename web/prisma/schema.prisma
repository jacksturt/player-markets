model UserProfile {
  id                           String     @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  email                        String?
  name                         String?
  username                     String?
  image                        String?
  marketCap                    Decimal    @default(0) @db.Decimal(27, 9)
  createdAt                    DateTime   @default(now())
  updatedAt                    DateTime   @default(now()) @updatedAt
  twitterId                    String?    @unique
  authId                       String?    @unique
  metadata                     Json?
  inviteId                     String?
  claimableCuratorRewards      BigInt?    @default(0)
  totalCuratorRewards          BigInt?    @default(0)
  score                        BigInt?    @default(0)
  totalAuthorRewards           BigInt?    @default(0)
  lastClaimedCuratorRewardsSig String?
  authorVaultPublicKey         String?    @unique
  bangersAuthored              Banger[]   @relation("author")
  bangersOwned                 Banger[]   @relation("owner")
  following                    Follow[]   @relation("Follower")
  followers                    Follow[]   @relation("Following")
  invited                      Invite[]   @relation("inviter")
  Launcher                     Launcher[]
  market                       Market[]   @relation("author")
  marketCreated                Market[]   @relation("curator")
  media                        Media[]
  authoredTrades               Trade[]    @relation("bangerAuthor")
  trades                       Trade[]
  invite                       Invite?    @relation("inviteUser", fields: [inviteId], references: [id])
  wallet                       Wallet[]

  @@schema("public")
}

model Invite {
  id           String        @id @default(cuid())
  code         String        @unique
  inviterId    String?       @db.Uuid
  createdAt    DateTime      @default(now())
  uses         Int           @default(1)
  inviter      UserProfile?  @relation("inviter", fields: [inviterId], references: [id], onDelete: Cascade)
  inviteUsedBy UserProfile[] @relation("inviteUser")

  @@schema("public")
}

model Follow {
  id          String      @id @default(cuid())
  followerId  String      @db.Uuid
  followingId String      @db.Uuid
  createdAt   DateTime    @default(now())
  follower    UserProfile @relation("Follower", fields: [followerId], references: [id])
  following   UserProfile @relation("Following", fields: [followingId], references: [id])

  @@unique([followerId, followingId])
  @@schema("public")
}

model Wallet {
  id               String       @id @default(cuid())
  publicKey        String       @unique
  userId           String?      @db.Uuid
  randomPrivateKey String?      @unique
  user             UserProfile? @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@schema("public")
}

model Media {
  id        String      @id @default(cuid())
  platform  String
  imageUrl  String
  createdAt DateTime    @default(now())
  name      String
  authorId  String      @db.Uuid
  bangers   Banger[]
  market    Market?
  author    UserProfile @relation(fields: [authorId], references: [id], onDelete: Cascade)

  @@schema("public")
}

model Market {
  id                   String      @id @default(cuid())
  createdAt            DateTime    @default(now())
  mediaId              String      @unique
  supply               Int
  buyPrice             Decimal     @db.Decimal(18, 9)
  sellPrice            Decimal?    @db.Decimal(18, 9)
  marketCap            Decimal?    @db.Decimal(27, 9)
  authorVaultPublicKey String
  signature            String?
  authorId             String      @db.Uuid
  metadata             Json?
  curatorId            String      @db.Uuid
  tweetId              String      @unique
  collectionPublicKey  String      @unique
  metadataURL          String      @unique
  lookupTableAddress   String      @unique
  poolPublicKey        String?     @unique
  bangers              Banger[]
  author               UserProfile @relation("author", fields: [authorId], references: [id], onDelete: Cascade)
  curator              UserProfile @relation("curator", fields: [curatorId], references: [id], onDelete: Cascade)
  media                Media       @relation(fields: [mediaId], references: [id], onDelete: Cascade)
  trades               Trade[]

  @@schema("public")
}

model Trade {
  id               String      @id @default(cuid())
  marketId         String
  type             TradeSide   @default(BUY)
  createdAt        DateTime    @default(now())
  price            Decimal     @db.Decimal(18, 9)
  bangerId         String
  userId           String      @db.Uuid
  authorId         String      @db.Uuid
  supplyAfterTrade Int
  txId             String
  incrementId      Int         @default(autoincrement())
  author           UserProfile @relation("bangerAuthor", fields: [authorId], references: [id], onDelete: Cascade)
  banger           Banger      @relation(fields: [bangerId], references: [id], onDelete: Cascade)
  market           Market      @relation(fields: [marketId], references: [id], onDelete: Cascade)
  user             UserProfile @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([txId, id])
  @@index([createdAt(sort: Desc)])
  @@index([txId])
  @@schema("public")
}

model Banger {
  id            String      @id @default(cuid())
  marketId      String
  mediaId       String
  createdAt     DateTime    @default(now())
  hasBeenBurned Boolean     @default(false)
  burnedAt      DateTime?
  ownerId       String      @db.Uuid
  authorId      String      @db.Uuid
  rarity        String?
  address       String      @unique
  author        UserProfile @relation("author", fields: [authorId], references: [id], onDelete: Cascade)
  market        Market      @relation(fields: [marketId], references: [id], onDelete: Cascade)
  Media         Media       @relation(fields: [mediaId], references: [id], onDelete: Cascade)
  owner         UserProfile @relation("owner", fields: [ownerId], references: [id], onDelete: Cascade)
  trades        Trade[]

  @@schema("public")
}

model Launcher {
  id          String      @id @default(cuid())
  tweetId     String      @unique
  launcherId  String      @db.Uuid
  UserProfile UserProfile @relation(fields: [launcherId], references: [id], onDelete: Cascade)

  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn
  phone

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}

enum TradeSide {
  BUY
  SELL

  @@schema("public")
}
