{
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "cd your-subfolder-name && yarn install",
        "nixpacksConfigPath": "nixpacks.toml"
    },
    "deploy": {
        "startCommand": "cd your-subfolder-name && yarn start",
        "healthcheckPath": "/metrics",
        "healthcheckTimeout": 100
    }
}